  "client_script": "frappe.web_form.on('after_load', () => {\n    const urlParams = new URLSearchParams(window.location.search);\n    const flight = urlParams.get('flight');\n\n    if (flight) {\n        frappe.web_form.set_value('flight', flight);\n\n        \n        frappe.call({\n            method: \"frappe.client.get_value\",\n            args: {\n                doctype: \"Flight\",\n                filters: { name: flight },\n                fieldname: [\"ticket_price\"]\n            },\n            callback: function(r) {\n                if (r.message) {\n                    frappe.web_form.set_value('flight_price', r.message.ticket_price);\n                }\n            }\n        });\n    }\n});\n\n\n\nfrappe.web_form.after_save = () => {\n    // Give Frappe some time to render its default alert\n    setTimeout(() => {\n        // Hide Frappe's default success alert\n        const success_alert = document.querySelector('.web-form-success');\n        if (success_alert) {\n            success_alert.style.display = 'none';\n        }\n\n        // Replace the entire form container with custom success message\n        const form_container = document.querySelector(\".web-form-container\");\n        if (form_container) {\n            form_container.innerHTML = `\n                <div class=\"text-center p-5\" style=\"border: 1px solid #eee; border-radius: 8px; background: #fff;\">\n                    <div style=\"font-size: 48px; color: #28a745;\">✅</div>\n                    <h2 style=\"margin-top: 10px;\">Ticket Booked</h2>\n                    <p>You will receive the details in your email soon</p>\n                    <a href=\"${window.location.pathname}\" class=\"btn btn-primary mt-4\">Submit another response</a>\n                </div>\n            `;\n        }\n    }, 300); // Delay ensures this runs after Frappe’s alert is shown\n};",
  "condition_json": "[]",
  "custom_css": "<style>\n.success-message {\n    padding: 30px;\n    border: 1px solid #ddd;\n    border-radius: 8px;\n    max-width: 500px;\n    margin: 0 auto;\n    background: #f9f9f9;\n}\n.success-message .btn {\n    background-color: #4CAF50;\n    color: white;\n    text-decoration: none;\n    padding: 10px 20px;\n    border-radius: 6px;\n    display: inline-block;\n}\n</style>",
